#include <iostream>
using namespace std;

int main (){//begin 

    //numeric luas, P, L 
    double Luas, P, L;
    //Display, "Masukan Panjangnya : "
    cout << "Masukkan Panjangnya : ";
    //Accept P 
    cin >> P;
    //Display, "Masukan Lebarnya : "
    cout << "MAsukkan Lebarnya : ";
    //Accept L
    cin >> L;
    //Compute Luas = P*L
    Luas = P*L;
    //Display, "Luasnya : " + Luas 
    cout << "Luasnya : " << Luas;
}//end